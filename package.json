{
  "name": "stylish",
  "version": "1.0.0",
  "description": "Dark themes for userstyles.org",
  "main": "sass/",
  "scripts": {
    "build-css": "node-sass --include-path scss scss/vsts.scss css/vsts.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "start": "npm run watch-css"
